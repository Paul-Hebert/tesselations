<svg 
  xmlns='http://www.w3.org/2000/svg' 
  xmlns:xlink='http://www.w3.org/1999/xlink' 
  width='100' 
  height='100' 
  viewBox='-200 -200 400 400'
>
  <path stroke="{{stroke}}" fill="{{fill}}" d="{{commands}}" id="root"></path>
  {{#each usePositions}}
    <use href="#root" x="{{x}}" y="{{y}}"/>
  {{/each}}
</svg>
